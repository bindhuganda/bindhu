#include<stdio.h>
int main()
{
	int a,b,c;
	printf("enter the year=");
	scanf("%d",&a);
	printf("enter the date=");
	scanf("%d",&b);
	printf("enter the month=");
	scanf("%d",&c);
	if(a%4==0)
	{
		printf("the date is leap year");
	}
	else
	{
		printf("the date is not a leap year");
		
	}
	
}

    
    
    TEST CASE
    enter the year=2002
enter the date=12
enter the month=4
the date is not a leap year
    
